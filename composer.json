{
    "name": "boxspaced/cms-skeleton-application",
    "require": {
        "robmorgan/phinx": "0.6.4",
        "zendframework/zend-mvc-i18n": "^1.0",
        "boxspaced/cms-account-module": "dev-rest-api as 1.9",
        "boxspaced/cms-asset-module": "dev-rest-api as 1.9",
        "boxspaced/cms-block-module": "dev-rest-api as 1.9",
        "boxspaced/cms-helpdesk-module": "dev-rest-api as 1.9",
        "boxspaced/cms-item-module": "dev-rest-api as 1.9",
        "boxspaced/cms-menu-module": "dev-rest-api as 1.9",
        "boxspaced/cms-search-module": "dev-rest-api as 1.9",
        "boxspaced/cms-slug-module": "^1.0",
        "boxspaced/cms-standalone-module": "dev-rest-api as 1.9",
        "boxspaced/cms-versioning-module": "^1.0",
        "boxspaced/cms-workflow-module": "dev-rest-api as 1.9",

        "boxspaced/cms-core-module": "dev-rest-api as 1.9"
    },
    "require-dev": {
        "phing/phing": "^2.14",
        "behat/behat": "^2.5",
        "behat/mink-extension": "^1.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.2",
        "fabpot/goutte": "^1.0",
        "zendframework/zend-component-installer": "^0.3.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-developer-tools": "^1.0",
        "stefano/stefano-db-profiler": "^0.2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin/",
        "process-timeout": 1200
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-account-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-asset-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-block-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-helpdesk-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-item-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-menu-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-search-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-slug-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-standalone-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-versioning-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-workflow-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/cms-core-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/entity-manager-module"
        },
        {
            "type": "vcs",
            "url": "https://github.com/boxspaced/entity-manager"
        }
    ]
}
